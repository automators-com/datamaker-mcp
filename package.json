{
  "name": "@automators/datamaker-mcp",
  "version": "0.2.6",
  "description": "The Automators DataMaker MCP server.",
  "main": "./build/index.js",
  "type": "module",
  "bin": {
    "datamaker-mcp": "./build/index.js",
    "@automators/datamaker-mcp": "./build/index.js"
  },
  "scripts": {
    "build": "tsc && chmod 755 build/index.js",
    "start": "dotenv -e .env -- node build/index.js",
    "dev": "dotenv -e .env -- sh -c 'pnpm dlx @modelcontextprotocol/inspector -e PORT=\"$PORT\" -e DATAMAKER_URL=\"$DATAMAKER_URL\" -e AUTOMATORS_AUTH_JWT=\"$AUTOMATORS_AUTH_JWT\" -e DATAMAKER_API_URL=\"$DATAMAKER_API_URL\" -e S3_TOKEN=\"$S3_TOKEN\" -e S3_ACCESS_KEY_ID=\"$S3_ACCESS_KEY_ID\" -e S3_SECRET_ACCESS_KEY=\"$S3_SECRET_ACCESS_KEY\" -e S3_URL=\"$S3_URL\" -e S3_BUCKET_NAME=\"$S3_BUCKET_NAME\" node build/index.js'",
    "prepublishOnly": "pnpm build",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm build && changeset publish"
  },
  "files": [
    "build"
  ],
  "keywords": [
    "mcp",
    "datamaker",
    "synthetic-data",
    "ai",
    "model-context-protocol"
  ],
  "author": "Automators",
  "license": "MIT",
  "packageManager": "pnpm@10.11.0",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.864.0",
    "@aws-sdk/s3-request-presigner": "3.864.0",
    "@hono/mcp": "0.1.1",
    "@hono/node-server": "1.18.2",
    "@modelcontextprotocol/sdk": "^1.11.3",
    "dotenv": "17.2.1",
    "hono": "4.9.1",
    "node-fetch": "3.3.2",
    "tiktoken": "^1.0.22",
    "xmldom": "0.6.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.4",
    "@types/node": "^22.15.18",
    "@types/xmldom": "0.1.34",
    "dotenv-cli": "^8.0.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
